
<div class="container-fluid">
    @* Datos generales *@
    <div class="row">
        <div class="col-md-12">
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Datos Generales</h3>
                    <div class="card-tools">
                        <button class="btn btn-tool" type="button" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body">
                    <div class="form-group">
                        <label for="txtNombre">Nombre:</label>
                        <input type="text" id="txtNombre" placeholder="Nombre Empresa SA de CV" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label for="txtDireccion">Direccion:</label>
                        <input type="text" id="txtDireccion" class="form-control" />
                    </div>
                    <div class="row">
                        <div class="col form-group">
                            <label for="txtNumInt">Número Interior:</label>
                            <input type="text" id="txtNumInt" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtNumInt">Número Exterior:</label>
                            <input type="text" id="txtNumExt" class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col form-group">
                            <label for="ddlCiudad">Ciudad:</label>
                            <select id="ddlCiudad" class="form-control">
                                <option value="Seleccionar">Seleccionar</option>
                            </select>
                        </div>
                        <div class="col form-group">
                            <label for="ddlEstado">Estado:</label>
                            <select id="ddlEstado" class="form-control">
                                <option value="Seleccionar">Seleccionar</option>
                            </select>
                        </div>
                        <div class="col form-group">
                            <label for="ddlPais">País:</label>
                            <select id="ddlPais" class="form-control">
                                <option value="Seleccionar">Seleccionar</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col form-group">
                            <label for="txtCP">CP:</label>
                            <input type="text" id="txtCP" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtRFC">RFC:</label>
                            <input type="text" id="txtRFC" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtNSS">NSS:</label>
                            <input type="text" id="txtNSS" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtIRS">IRS:</label>
                            <input type="text" id="txtIRS" class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col form-group">
                            <label for="txtTelefono">Telefono:</label>
                            <input type="text" id="txtTelefono" class="form-control" placeholder="(___) ___-____" />
                        </div>
                        <div class="col form-group">
                            <label for="txtTelefonod">Telefono 2:</label>
                            <input type="text" id="txtTelefonod" class="form-control" placeholder="(___) ___-____" />
                        </div>
                        <div class="col form-group">
                            <label for="txtUR">URL:</label>
                            <input type="text" id="txtUR" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtFAX">FAX:</label>
                            <input type="text" id="txtFAX" class="form-control" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @* Otros datos *@
    <div class="row">
        <div class="col-md-12">
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Otros datos</h3>
                    <div class="card-tools">
                        <button class="btn btn-tool" type="button" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col form-group">
                            <label for="lblCliDesde">Cliente desde:</label>
                            <label id="lblCliDesde" class="form-control"></label>
                        </div>
                        <div class="col form-group">
                            <label for="txtManejosEx">CliManejosEx:</label>
                            <input type="number" id="txtManejosEx" step="0.01" value="0.0" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtServCompEx">CliServCompEx:</label>
                            <input type="number" id="txtServCompEx" step="0.01" value="0.0" class="form-control" />
                        </div>
                        <div class="col form-group">
                            <label for="txtServCompIm">CliServCompIm:</label>
                            <input type="number" id="txtServCompIm" step="0.01" value="0.0" class="form-control"/>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col form-group">
                            <label>CliHandlingIm:</label>
                            <input type="text" id="txtCliHandlingIm" value="" class="form-control"/>
                        </div>
                        <div class="col form-group">
                            <label>CliAcumTransAñoP:</label>
                            <input type="number" id="txtCliAcumTransAñoP" step="1" value="0" min="0" class="form-control"/>
                        </div>
                        <div class="col form-group">
                            <label>CliAcumTransAñoD:</label>
                            <input type="number" id="txtCliAcumTransAñoD" step="1" value="0" min="0" class="form-control"/>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col form-group">
                            <label>CliAcumPesos</label>
                            <input type="text" id="txtCliAcumPesos" value="" class="form-control"/>
                        </div>
                        <div class="col form-group">
                            <label>CliAcumDolar</label>
                            <input type="text" id="txtCliAcumDolar" value="" class="form-control"/>
                        </div>
                        <div class="col form-group">
                            <label>CliPagaPesos</label>
                            <input type="text" id="txtCliPagaPesos" value="" class="form-control"/>
                        </div>
                        <div class="col form-group">
                            <label>CliPagaDolar</label>
                            <input type="text" id="txtCliPagaDolar" value="" class="form-control"/>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <label>CliTotTrans</label>
                            <input type="number" id="txtCliTotTrans" value="" class="form-control"/>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    @* Lo que sea *@
    <div class="row">
